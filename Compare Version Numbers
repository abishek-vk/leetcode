class Solution:
    def compareVersion(self, version1: str, version2: str) -> int:
        a = version1.split(".")
        b = version2.split(".")
        while len(a)<len(b):
            a.append("0")
        while len(a)>len(b):
            b.append("0")
        for i in range(len(a)):
            if int(a[i])<int(b[i]):
                return -1
            if int(a[i])>int(b[i]):
                return 1
        return 0   
